  "description": "Enterprise-grade frontend for OpenClaw browser automation agents",
  "scripts": {
    "dev": "concurrently --names SERVER,CLIENT,TRADER --prefix-colors cyan,magenta,yellow \"node server/index.js\" \"npx vite\" \"npm run dev:trader\"",
    "dev:all": "concurrently --names SERVER,CLIENT,TRADER,GATEWAY --prefix-colors cyan,magenta,yellow,green \"node server/index.js\" \"npx vite\" \"npm run dev:trader\" \"openclaw gateway start\"",
    "dev:server": "node server/index.js",
    "dev:client": "npx vite",
    "dev:trader": "cd C:\\Users\\SPilcher\\openclaw-trader && npx tsx watch apps/trader-service/src/server.ts",
    "build": "vite build",
    "build:trader": "tsc -p services/trader-service/tsconfig.json",
    "seed": "node scripts/seed-demo.js",
    "setup": "bash scripts/setup.sh",
    "test": "vitest run",
    "preview": "vite preview"
  },
