{
  "name": "trader-service",
  "version": "0.1.0",
  "private": true,
  "description": "OpenClaw Trading Service - Core trading engine with risk management",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "tsx test-system.ts",
    "test:e2e": "tsx test-e2e.ts",
    "test:integration": "tsx test-integration.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "tsx src/db/migrations/run.ts",
    "db:seed": "tsx src/db/migrations/seed.ts",
    "typecheck": "tsc --noEmit",
    "cli": "tsx cli.ts",
    "monitor": "tsx monitor.ts",
    "strategy": "tsx run-strategy.ts",
    "verify": "tsx verify-phase2.ts",
    "compare": "tsx strategy-compare.ts",
    "test:polymarket": "tsx test-polymarket.ts"
  },
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^3.0.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-boss": "^9.0.3",
    "prom-client": "^15.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
